.head{
  height:100rpx;
  display: flex;
  justify-content: space-between;
  background-color:#fff;
}
.head-ico{
  width:120rpx;
  height:100%;
  box-sizing: border-box;
  padding:20rpx 30rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.head-ico-line{
  height:2rpx;
  width:100%;
  background-color:#999;
}
.head-ico-top{
  margin-bottom:20rpx;
}
.logo{
  height:60rpx;
  width:188rpx;
  padding-top:20rpx;
}
.head-log-btn{
  padding:20rpx 30rpx;
  width:60rpx;
  height:60rpx;
}

.head-select-wrap{
  position:fixed;
  width:100%;
  top:102rpx;
  left:0;
}
.head-select{
  width:100%;
  height:80rpx;
  display: flex;
  background:#fff;
}
.head-select-item{
  width:33%;
  display: flex;
  align-items: center;
  justify-content: center;
}
.head-select-item text{
  margin-right:20rpx;
}
.head-select-item image{
  width:24rpx;
  height:12rpx;
}