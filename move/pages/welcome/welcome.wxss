page{
  height:100%;
  background-color:#b3d4db;
}
.box{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.avatarImg{
  width:200rpx;
  height:200rpx;
  margin-top:100rpx;
}
.userName{
  font-size:32rpx;
  font-weight: bold;
  margin-top:200rpx;
}
.motoBox{
  width:200rpx;
  height:80rpx;
  border:1px solid #405f80;
  border-radius:5px;
  text-align:center;
  margin-top:200rpx;
}
.moto{
  font-size:22rpx;
  font-weight: bold;
  color:#405f80;
  line-height:80rpx;
}